{
    "compilerOptions": {
        "allowJs": false,
        "allowUnreachableCode": false,
        "alwaysStrict": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "module": "commonjs",
        "noUnusedLocals": true,
        "outDir": "dist",
        "removeComments": true,
        "rootDir": "src",
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "target": "es2017"
    }
}
